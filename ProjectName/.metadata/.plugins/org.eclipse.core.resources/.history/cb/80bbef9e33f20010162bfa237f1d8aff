public class Point {
	private float x, y, z;

	public Point() {
		setX(0);
		setY(0);
		setZ(0);
	}

	public Point(float x, float y, float z) {
		setX(x);
		setY(y);
		setZ(z);
	}

	public float getX() {
		return x;
	}

	protected void setX(float x) {
		this.x = x;
	}

	public float getY() {
		return y;
	}

	protected void setY(float y) {
		this.y = y;
	}

	public float getZ() {
		return z;
	}

	protected void setZ(float z) {
		this.z = z;
	}

	public String toString() {
		return String.format("(%.2f, %.2f, %.2f)", x, y, z);
	}
}
